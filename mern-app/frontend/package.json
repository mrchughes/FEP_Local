        "@testing-library/user-event": "^14.5.2",
        "cypress": "^13.6.0",
        "cypress-file-upload": "^5.0.8",
        "jest": "^27.5.1",
        "msw": "^2.2.5",
        "start-server-and-test": "^2.0.0"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
